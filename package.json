    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.0",
    "sass": "^1.62.0",
    "vite": "^4.3.1",
    "vite-plugin-istanbul": "^4.0.1",
